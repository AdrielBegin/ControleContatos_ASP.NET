@{
    ViewData["Title"] = "Editar Contato";
}
<div class="">

    <h1 class="display-4 text-center">Cadastradar Contato</h1>

    <form>
        <div class="mb-3">
            <label for="nome" class="form-label">Nome</label>
            <input type="text" class="form-control">
        </div>
        <div class="mb-3">
            <label for="email" class="form-label">Email address</label>
            <input type="email" class="form-control">
        </div>
        <div class="mb-3">
            <label for="celular" class="form-label">Celular</label>
            <input type="number" class="form-control">
        </div>
        <button type="submit" class="btn btn-outline-primary btn-block">Alterar</button>
        <a role="button" class="btn btn-outline-secondary btn-block" asp-controller="ContatoController1" asp-action="Index">Voltar</a>
    </form>

</div>